extends  ../_skeleton

block variables
	- var codedBy = true

block title
	title Home

block page
	// Page Header
	include ../sections/_header-secondary
	='\n'
	
	// Slider
	include ../sections/_swiper-classic
	="\n"
	
	// About
	section.section.section-75.section-md-120.section-lg-120.section-xl-160.section-xl-bottom-180#about
		.shell.text-left
			.range
				.cell-sm-6.section-sm-40.section-lg-80.section-lg-bottom-110
					.divider-block
						h2 we are the leaders of the plastic products market
						h6 Since our incorporation in 1981, we've grown from a regional organization to a multi-faceted, national presence in the industry.
						p Our commitment to excellence has helped us grow into a globally recognized and trusted leader in injection molding. We tackle problems, test theories, and partner with the world’s best engineers so we can be the source for where innovation takes form. We have also maintained a steadfast commitment to customer service, protecting our clients.
				.cell-sm-5.cell-sm-preffix-1.bg-custom-image.bg-custom-image-right.bg-custom-image-3
					.custom-border
	="\n"
	
	section.section-75.section-md-120.section-xl-130.bg-darker
		.shell
			.range
				.cell-md-8
					.divider-block
						h2 WE PROVIDE TAILORED SOLUTIONS FOR YOUR BUSINESS
				.cell-md-4.cell-md-middle.text-center
					a.btn.btn-default.btn-default-white(href='#', data-toggle='modal', data-target='#modalContact') Contact Us
	='\n'
	
	// Counter
	section.section-75.section-md-120.section-xl-130.bg-wild-sand
		include ../sections/_section-counters
	="\n"
	
	// Services
	include ../sections/_section-services
	="\n"
	
	// Project
	section.section-50.section-lg-bottom-0#products
		.shell
			.range
				.cell-md-10.cell-lg-7.cell-xl-6
					.divider-block
						h2 Examples of finished products
						h6
							| We manufacture all kinds of plastic products for your individual
							br.veil.reveal-md-block
							="\t"
							| and corporate needs. Some of them are pictured below.
		
		.owl-carousel-wrap.text-center.wrap-fluid
			.owl-carousel(
				data-items='1'
				data-sm-items='2'
				data-md-items='3'
				data-lg-items='4'
				data-xl-items='5'
				data-xxl-items='5'
				data-stage-padding='0'
				data-loop='true'
				data-margin='15'
				data-mouse-drag="true"
				data-autoplay='true'
				data-dots='true'
				data-nav-custom=".owl-custom-navigation"
			)
				.owl-item
					.product.product-custom
						img.img-responsive(src='images/home-product-01-372x500.jpg', alt='', width='372', height='500')
						a(href='#')
							.product-content
								.small Small mechanical components
				.owl-item
					.product.product-custom
						img.img-responsive(src='images/home-product-03-372x500.jpg', alt='', width='372', height='500')
						a(href='#')
							.product-content
								.small Plastic household Buckets
				.owl-item
					.product.product-custom
						img.img-responsive(src='images/home-product-04-372x500.jpg', alt='', width='372', height='500')
						a(href='#')
							.product-content
								.small Consumer electronics
				.owl-item
					.product.product-custom
						img.img-responsive(src='images/home-product-05-372x500.jpg', alt='', width='372', height='500')
						a(href='#')
							.product-content
								.small Safety goggles
				.owl-item
					.product.product-custom
						img.img-responsive(src='images/home-product-06-372x500.jpg', alt='', width='372', height='500')
						a(href='#')
							.product-content
								.small Sets of Black Plastic Gears
			.owl-custom-navigation
				.owl-nav
					.owl-prev(data-owl-prev) Prev
					.owl-next(data-owl-next) Next
	='\n'
	
	// Brands
	include ../sections/_section_brands
	='\n'
	
	// Latest News
	section.section-75.section-sm-80.section-md-120.section-xl-150.bg-white#news
		.shell
			.divider-block
				h2 Latest News
			.range.range-sm-center.range-md-left
				.cell-lg-4.cell-xl-3.cell-xl-preffix-1.cell-sm-6
					article.blog-grid
						a.blog-grid-image(href='#')
							+jpg_responsive('blog-grid-01-610x250')
							.blog-grid-title industry news
						.blog-grid-content
							h5: a.text-primary(href='#') Introducing Polypropylene Waxes
							p At the very least, I always find it reassuring when we hear of any new plastics recycling effort. But, it’s downright inspirational when that effort is effectively converting...
							p.text-darker: time(datetime='2018') August 11, 2018
				.cell-lg-4.cell-xl-3.cell-xl-preffix-1.cell-sm-6.offset-top-40.offset-sm-top-0
					article.blog-grid
						a.blog-grid-image(href='#')
							+jpg_responsive('blog-grid-04-610x250')
							.blog-grid-title interesting facts
						.blog-grid-content
							h5: a.text-primary(href='#') How Plastics Influence Modern Sports
							p Plastics have revolutionised sports in recent years. From tracks on which Olympic athletes pursue new records to shoes, clothing, safety equipments...
							p.text-darker: time(datetime='2018') September 12, 2018
				.cell-lg-4.cell-xl-3.cell-xl-preffix-1.cell-sm-6.offset-top-40.offset-lg-top-0
					article.blog-grid
						a.blog-grid-image(href='#')
							+jpg_responsive('blog-grid-06-610x250')
							.blog-grid-title innovations
						.blog-grid-content
							h5: a.text-primary(href='#') Making Robots Safer Around People
							p So-called “collaborative” robots, or “cobots,” have captured the imagination of a number of plastics processors who are looking for automation that can operate safely in...
							p.text-darker: time(datetime='2018') November 21, 2018
				.cell-xs-12
	='\n'
	
	// Newsletter
	section.section.section-75.section-sm-80.section-md-100.bg-wild-sand
		.shell
			.range
				.cell-md-6
					.divider-block
						h2 Newsletter SignUp
						h6 Enter your email address to get the latest plastic industry news, events and special offers delivered right to your inbox.
				.cell-md-6.cell-xl-5.cell-xl-preffix-1.cell-md-bottom
					form.rd-mailform.text-left.rd-mailform-subscribe(data-form-output="form-output-global", data-form-type='subscribe', method='post', action='bat/rd-mailform.php')
						.form-group
							label.form-label.form-label-subscribe(for="footer-subscribe-email") Enter your e-mail
							input#footer-subscribe-email.form-control.form-control-subscribe(type="email" name="email" data-constraints="@Email @Required")
						button(type="submit").btn.btn-default.btn-default-white subscribe
	='\n'
	
	// Team
	include ../sections/_section-out-team
	='\n'
	
	// Contacts
	include ../sections/_section-form
